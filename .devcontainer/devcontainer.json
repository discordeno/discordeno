{
  "name": "Discordeno",
  "dockerFile": "Dockerfile",
  "settings": {
    "deno.enable": true,
    "deno.lint": false,
    "deno.unstable": true,
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "extensions": [
    "denoland.vscode-deno"
  ],
  "remoteUser": "vscode"
}
