{
    "minify": true,
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2022",
        "keepClassNames": true,
        "loose": true,
        "minify": {
            "compress": {
                "unused": true
            },
            "mangle": true
        }
    },
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "sourceMaps": "inline"
}